1.	Submit Game Users [PUT] [/participants]
	+ Request (application/json)
	{
	   "gameId":"102",
	   "name":"Game Name",
	   "active":"true",
	   "startDate":"03/04/2020",
	   "endDate":"15/04/2020",
	   "numWinners":"3",
	   "users":[
		  {
			 "username":"ramit"
		  },
		  {
			 "username":"chris"
		  },
		  {
			 "username":"lucas"
		  }
	   ]
	}
	+ Response 202 (application/json)
    {
        "accepted": "true",
    } 
 
