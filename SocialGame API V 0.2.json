4. Get Points Earned and Base Points [GET] [/energy/points]
	+ Request (application/json) 
	{
	   "gameId":"102",
	   "acknowledgementId":"10248181"
	}
	  
	+ Response 102 (application/json) 
	{
	   "gameId":"102",
	   "acknowledgementId":"10248181",
	   "message":"processing"
	}
	+ Response 200 (application/json)
	{
	   "gameId":"102",
	   "acknowledgementId":"10248181",
	   "message":"completed",
	   "users":[
	      {
	         "user":"ramit",
	         "basepoints":"1054",
	         "earnedpoints":"945"
	      },
	      {
	         "user":"lucas",
	         "basepoints":"1054",
	         "earnedpoints":"945"
	      }
	   ]
	}
    
